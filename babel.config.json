{
  "presets": [
    [
      "@babel/preset-env",
      { "targets": { "esmodules": true, "node": "current" } }
    ],
    "@babel/preset-typescript",
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    [
      "i18next-extract",
      {
        "locales": ["ru", "en"],
        "keyAsDefaultValue": true
      }
    ],
    "@babel/plugin-transform-typescript",
    "@babel/plugin-transform-runtime"
  ]
}